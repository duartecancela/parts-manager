{
    "openapi": "3.0.0",
    "info": {
        "title": "Parts Manager API",
        "version": "0.1"
    },
    "paths": {
        "/api/parts": {
            "get": {
                "tags": [
                    "index"
                ],
                "summary": "List parts.",
                "description": "List of all electronic parts",
                "operationId": "index",
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "part": {
                                            "type": "Object",
                                            "example": {
                                                "id": 1,
                                                "name": "BC545",
                                                "category": "Transistores",
                                                "quantity": 0
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "store"
                ],
                "summary": "Store part.",
                "description": "Store an electronic part",
                "operationId": "store",
                "requestBody": {
                    "description": "Insert part fields",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "category",
                                    "quantity"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "BC545"
                                    },
                                    "category_id": {
                                        "type": "string",
                                        "example": "1"
                                    },
                                    "description": {
                                        "type": "string",
                                        "example": "general propose transistor"
                                    },
                                    "quantity": {
                                        "type": "int",
                                        "example": "0"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "part": {
                                            "type": "Object",
                                            "example": {
                                                "id'": 1,
                                                "name": "BC545",
                                                "category_id": "1",
                                                "quantity": 0
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/parts/{id}": {
            "get": {
                "tags": [
                    "show"
                ],
                "summary": "Show part.",
                "description": "Show an electronic part",
                "operationId": "show",
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "part": {
                                            "type": "Object",
                                            "example": {
                                                "id": 5,
                                                "category_id": 2,
                                                "name": "250",
                                                "description": "1/4 W",
                                                "stock": 0,
                                                "created_at": "12/15/2021",
                                                "updated_at": "12/15/2021"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "put": {
                "tags": [
                    "update"
                ],
                "summary": "Update part",
                "description": "Update an electronic part",
                "operationId": "Upadte",
                "requestBody": {
                    "description": "Insert part fields",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "category_id",
                                    "quantity"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "250"
                                    },
                                    "category_id": {
                                        "type": "string",
                                        "example": "2"
                                    },
                                    "description": {
                                        "type": "string",
                                        "example": "1/2W"
                                    },
                                    "quantity": {
                                        "type": "int",
                                        "example": "0"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "part": {
                                            "type": "Object",
                                            "example": {
                                                "id": 5,
                                                "category_id": "2",
                                                "name": "250",
                                                "0": "1",
                                                "description": "1/2",
                                                "created_at": "12/15/2021",
                                                "updated_at": "01/27/2022"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "security": [
        []
    ]
}